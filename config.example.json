{
	"data-sinks": [
		{
			"type": "postgres",
			"enabled": true,
			"options": {
				"connectionString": "postgres://someuser:password@host/database"
			}
		},
		{
			"type": "udp",
			"enabled": false,
			"options": {
				"udp6": false,
				"host": "127.0.0.1",
				"port": 999
			}
		},
		{
			"type": "redis",
			"enabled": true,
			"options": {
				"connectionString": null,
				"excludeFields": [
					"profileurl"
				]
			}
		}
	],
	"excludeFields": [
		"avatarmedium",
		"avatarfull"
	],
	"broadcastIntervalSeconds": 30,
	"steamApiKey": "yourSteamApiKeyHere",
	"enableDebugConsoleMessages": false,
	"broadcastOnAllInterfaces": true,
	"broadcastOnInterfaces": []
}